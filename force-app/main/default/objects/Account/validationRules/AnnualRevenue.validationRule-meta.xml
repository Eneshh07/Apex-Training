<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AnnualRevenue</fullName>
    <active>false</active>
    <errorConditionFormula>OR( AnnualRevenue &lt; 0 , AnnualRevenue &gt; 100000000000)</errorConditionFormula>
    <errorDisplayField>AnnualRevenue</errorDisplayField>
    <errorMessage>Annual Revenue cannot exceed 100 billion</errorMessage>
</ValidationRule>
